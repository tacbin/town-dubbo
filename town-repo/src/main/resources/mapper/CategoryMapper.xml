<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tacbin.town.repo.mapper.ICategoryMapper">

    <select id="queryProductsOfCategory" resultType="com.tacbin.town.repo.entity.Category">
        SELECT * FROM user_extend WHERE USERID = (SELECT ID FROM userinfo WHERE `USER_NAME` = #{userName})
    </select>


</mapper>